    "build-and-deploy": "npm run build && npm run deploy",
    "deploy": "node ./scripts/deploy.js",
    "postinstall": "./scripts/postinstall.sh",
    "test": "react-scripts test --transformIgnorePatterns --testPathIgnorePatterns=.data.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
